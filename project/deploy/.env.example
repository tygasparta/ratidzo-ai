# Ratidzo Health AI - Environment Variables
# Rename this file to .env and fill in your actual values

# Server configuration
PORT=3000
NODE_ENV=production
LOG_TO_FILE=true
LOG_LEVEL=info

# API Keys
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Timeout settings (in milliseconds)
API_TIMEOUT=30000

# Domain settings
DOMAIN=ratidzo.com
ALLOWED_ORIGINS=https://ratidzo.com,https://www.ratidzo.com

# Security
SESSION_SECRET=generate_a_strong_random_string_here 